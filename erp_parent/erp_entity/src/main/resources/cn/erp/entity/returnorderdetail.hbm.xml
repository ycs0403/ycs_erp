<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.erp.entity.Returnorderdetail" table="returnorderdetail">
		<id name="uuid">
		<!-- 主键序列生成策略 -->
			<generator class="org.hibernate.id.SequenceGenerator">
				<param name="sequence">RERURNORDERDETAIL_SEQ</param>
			</generator>
		</id>
		<property name="goodsuuid" />
		<property name="goodsname" />
		<property name="price" />
		<property name="num" />
		<property name="money" />
		<property name="endtime" />
		<property name="ender" />
		<property name="storeuuid" />
		<property name="state" />
		<property name="ordersuuid" />
	</class>
	
</hibernate-mapping>
